<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=<device-width>, initial-scale=1.0" />
    <title>Schedule</title>
    <link rel="stylesheet" href="/style.css" />
  </head>
  <body>
    <nav>
      <div class="nav-wrapper blue lighten-1">
        <a href="#" class="Mrlogo left"> </a>
        <a href="#" class="brand-logo center">Schedule App</a>
        <ul class="right" id="nav-mobile">
          <li><a href="/home/login">Home</a></li>
          <li><a href="/logout">Logout</a></li>
        </ul>
      </div>
    </nav>
    <main>
      <table>
        <thead>
          <tr>
            <th>User</th>
            <th>Day</th>
            <th>Start at</th>
            <th>End at</th>
          </tr>
        </thead>
        <tbody>
          <% for (let i = 0; i < schedules.length; i++) { %>
          <tr>
            <td><%= schedules[i].username %></td>
            <td><%= schedules[i].day %></td>
            <td><%= schedules[i].start_time %></td>
            <td><%= schedules[i].end_time %></td>
          </tr>
          <% } %>
        </tbody>
      </table>
      <a href="/schedules/new">New Schedule</a>
    </main>
  </body>
</html>
